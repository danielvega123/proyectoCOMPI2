funcion prueba(){
	Dimv valor : numeroMayor(1,2);
	imprimir(valor);
}

funcion numeroMayor(x,y){
si(x > y){
Retornar x;
}sino{
Dimv a,b;
Para( a : 10; a > 0; --){
	Para( b : 10; b > 0; --){
			imprimir(a+"*"+b+"="+a*b);
	}
}
retornar y;
}
}

