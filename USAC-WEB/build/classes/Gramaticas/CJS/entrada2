funcion tipo_operacion(tipo, num1 ,num2){
	SELECCIONA (tipo){
		caso "+":
			retornar num1 + num2;
		caso "-":
			retornar num1 - num2;	
					
	}
}

funcion factorial(num){
	si(num == 0){
		retornar 1;
	}sino{
		retornar factorian(num) * factorial(num - 1); 
	}
}

