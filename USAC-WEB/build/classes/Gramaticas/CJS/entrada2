funcion numeroMayor(x,y){
si(x > y){
Retornar x;
}sino{
Dimv a,b;
Para( a : 10; a > 0; --){
	Para( b : 10; b > 1; --){
		imprimir(a+"*"+b+"="+a*b);
		SELECCIONA (a+b){
		caso 10:
			mientras((a+b)%10==0){
					imprimir("entro al while"+"debido a que: "+a+"+"+b+"="+(a+b));
					detener;
			}		
		caso 12: 
			mientras((a+b)%12==0){
					imprimir("entro al while"+"debido a que: "+a+"+"+b+"="+(a+b));
					detener;
			}
		casO 20:
			mientras((a+b)%20==0){
					imprimir("entro al while"+"debido a que: "+a+"+"+b+"="+(a+b));
					detener;
			}
		defecto:
			si(a*b==1){
				detener;
			}sino{
				imprimir("entro al while"+"debido a que: "+a+"+"+b+"="+(a+b));
			}
		}	
	}
}
retornar y;
}
}




