Dimv b;
funcion numeroPar(x){
	Dimv a,b;
	Para( a : 100; a > 0; --){
		SELECCiona (a%2){
			caso 0:
				imprimir("Es par");

			caso 1:
				imprimir("no me agarren we");
			caso 2:
				imprimir("ya me agarraron we xd");
				detener;
		}
		si(x > a){
			imprimir("me salgo del for porque" + x + " > "+a);
			retornar a;
		}sino{
			imprimir(a);
		}
	}
}

funcion factorial(num){
	si(num <= 0){
		retornar 1;
	}sino{
		retornar num*factorial(num-2) * num*factorial(num - 1); 
	}
}

